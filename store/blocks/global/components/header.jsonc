{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": ["sticky-layout#desktop", "flex-layout.row#desktopicons"]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": ["flex-layout.row#desktop"]
  },
  "flex-layout.row#desktop": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": false,
      "fullWidth": true,
      "blockClass": "FLEX-HEADER-DESKTOP",
      "marginTop": 7
    },
    "children": [
      "flex-layout.col#drawer",
      "flex-layout.col#logo-desktop",
      "search-bar",
     // "flex-layout.col#spacer",
      "flex-layout.row#buttons",
      "minicart.v2",
      "login"
    ]
  },
  "search-bar": {
    "props": {
      "blockClass": ["searchbar-header"]
    }
  },
  "flex-layout.col#drawer": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center"
    },
    "children": ["drawer"]
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center"
    },
    "children": ["logo"]
  },
  "logo": {
    "props": {
      "title": "LogoITG",
      "href": "/",
      "url": "assets/img/iconblack.png",
      "width": "auto"
    }
  },
  "header-layout.mobile": {
    "children": ["sticky-layout#mobile"]
  },
  "sticky-layout#mobile": {
    "children": ["flex-layout.row#mobile"]
  },
  "flex-layout.row#mobile": {
    "children": [
      "drawer",
      "logo",
      "flex-layout.col#spacer",
      "login",
      "minicart.v2"
    ],
    "props": {
      "blockClass": "main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "drawer": {
    "children": []
  },
  "login": {
    "props": {
      "blockClass":["login-desktop"]
    }
  },
  "flex-layout.row#buttons": {
    "children": [
      "link#trend",
      "link#monthcollection",
      "link#januaryoff",
      "link#cybermonday",
      "link#neversay"
    ],
    "props": {
      "blockClass": "textbuttons"
    }
  },
  "link#trend": {
    "props": {
      "label": "Tendencias de it globers",
      "href": "/",
      "blockClass":["hdr-link-trend"]
    }
  },
  "link#monthcollection": {
    "props": {
      "label": "Coleccion del mes",
      "href": "/",
      "blockClass":["hdr-links"]
    }
  },
  "link#januaryoff": {
    "props": {
      "label": "Ofertas enero",
      "href": "/",
      "blockClass":["hdr-links"]
    }
  },
  "link#cybermonday": {
    "props": {
      "label": "Cyber Monday",
      "href": "/",
      "blockClass":["hdr-links"]
    }
  },
  "link#neversay": {
    "props": {
      "label": "Never say never",
      "href": "/",
      "blockClass":["hdr-links"]
    }
  }
}
